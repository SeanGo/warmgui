					<txtbkg-color-alpha>28, 0, 0, 1.0</txtbkg-color-alpha>
					<fixcol-color-alpha>28, 0, 0, 1.0</fixcol-color-alpha>
					<fixcol-bkg-color-alpha>28, 0, 0, 1.0</fixcol-bkg-color-alpha>
